<script type="text/javascript">
const apiKey = "42a2ae40-6769-11ec-99e0-67265e3ceb10";
const url = "https://api.freegeoip.app/json/?apikey=" + apiKey;

const embeddedForm = "#us-and-uk-embedded-donation-form";
const modalButton = ".us-and-uk-donate-button";

jQuery(document).ready(async function($){
  	const hasEmbeddedForm = $(embeddedForm).length > 0;
    const hasModalForm = $(modalButton).length > 0;
  
  	// don't run this script if the form element or donate button doesn't exist on the page
  	if (!hasEmbeddedForm && !hasModalForm) {
    	return;
    }
  
  	let isUkIpAddress = false; 
    try {
        let res = await fetch(url);
        res = await res.json();
      	if (res.country_code === "GB") {
        	isUkIpAddress = true;
        }
    } catch (error) {
        console.error(error);
    }
	
	let classyCampaignId = "308889";
	if (
		window.location.pathname === "/thank-you-child-slavery-pledge/" ||
		window.location.pathname === "/rescue-183-enslaved-children/"
	) {
		classyCampaignId = "406817";
	}
	
	if (
		window.location.pathname === "/realestate/"
	) {
		classyCampaignId = "412870";
	}
  
  	// if UK user, show Donorbox form
  	if (isUkIpAddress) {
      	// console.log("UK user");
      
      	var donorboxScript = window.document.createElement("script");
        donorboxScript.setAttribute("paypalExpress", "false");
        donorboxScript.setAttribute("src", "https://donorbox.org/widget.js");
        window.document.body.appendChild(donorboxScript);
      
      	if (hasEmbeddedForm) {
          	// console.log("\tembedded form");
            $(embeddedForm).append(`
                <iframe
                    src='https://donorbox.org/embed/from-injustice-to-influence-1'
                    name='donorbox'
                    allowpaymentrequest='allowpaymentrequest'
                    seamless='seamless'
                    frameborder='0'
                    scrolling='no'
                    height='900px'
                    width='100%'
                    style='max-width: 425px; min-width: 250px; max-height:none!important'
                ></iframe>
            `);
        }
      
      	if (hasModalForm) {
          	// console.log("\tmodal form");
          
          	// create modal, initially hidden
          	$("body").append(`
				<div>
					<div class="donorbox-modal">
						<div class="donorbox-modal-dimmed-bg"></div>
						<div class="modal-content">
							<div class="donorbox-modal-close-button">×</div>
                            <iframe
                                src='https://donorbox.org/embed/from-injustice-to-influence-1'
                                name='donorbox'
                                allowpaymentrequest='allowpaymentrequest'
                                seamless='seamless'
                                frameborder='0'
                                scrolling='no'
                            ></iframe>
						</div>
					</div>
					<style>
						.donorbox-modal {
                            overflow: hidden;
                            position: fixed;
                            top: 0;
                            left: 0;
                            right: 0;
                            bottom: 0;
                            z-index: 10000;
                            padding-top: 32px;
                            display: flex;
                            align-items: center;
                            justify-content: center;
                            opacity: 0;
                            pointer-events: none;
                            visibility: hidden;
						}
						.donorbox-modal.open {
							opacity: 1;
                            pointer-events: auto;
                            visibility: visible;
						}
						.donorbox-modal.open * {
							pointer-events: auto;
						}
						.donorbox-modal-dimmed-bg {
							position: absolute;
                            top: 0;
                            left: 0;
                            right: 0;
                            bottom: 0;
                            background: rgba(0, 0, 0, 0.65);
						}
						.donorbox-modal .modal-content {
							position: absolute;
						}
						.donorbox-modal-close-button {
							color: white;
                            position: absolute;
                            right: -32px;
                            top: -32px;
                            display: inline-block;
                            cursor: pointer;
							padding: 0;
                            border-width: 0;
                            text-shadow: 1px 1px 1px rgb(0 0 0 / 25%);
							font: lighter 28px/1 sans-serif;
						}
						@media (max-width: 460px) {
							.donorbox-modal-close-button {
								right: 4px;
							}
						}
						.donorbox-modal iframe {
							position: relative;
							height: auto;
							max-height: 765px !important;
                            width: 100vw;
                            max-width: 425px;
							min-width: 250px;
							margin-bottom: 0;
						}
					</style>
				</div>
			`);
        }
    }
  
  	// if non-UK user, show Classy form
  	else {
      	// console.log("non-UK user");
        if (hasEmbeddedForm) {
          	// console.log("\tembedded form");
    		$(embeddedForm).append(`
				<iframe
					height='520'
					src='https://invest.manyhopes.org/give/${classyCampaignId}/#!/donation/checkout?eg=true'
					allowpaymentrequest='true'
				></iframe>
			`);
        }
      
      	if (hasModalForm) {
          	// console.log("\tmodal form");
          	window.egProps = {
				modal: {
					campaigns: [{
						campaignId: classyCampaignId,
						elementSelector: modalButton + " a, " + modalButton + " a span",
						urlParams: readURLParams(),
            		}],
				},
				
			};
			window.document.body.appendChild(makeEGScript());
  
			// Create the embed script
			function makeEGScript() {
				var egScript = window.document.createElement("script");
				egScript.setAttribute("type", "text/javascript");
				egScript.setAttribute("async", "true");
				egScript.setAttribute("src", "https://sdk.classy.org/embedded-giving.js");
				return egScript;
			}

			// Read URL parameters and pass them to the donation page in the embedded modal
			function readURLParams() {
				var searchParams = new URLSearchParams(location.search);

      			// Add or remove url parameters that you want to pass over to the embed modal.
				var validUrlParams = ["c_src", "c_src2"];
				return validUrlParams.reduce(function (urlParamsSoFar, validKey) {
					var value = searchParams.get(validKey);
					return value === null
						? urlParamsSoFar
						: { ...urlParamsSoFar, [validKey]: value };
				}, {});
			}
        }
    }
});

// open donorbox modal on button click
jQuery(modalButton).click(function() {
  	// console.log("clicked on modal button");
  	if (jQuery(".donorbox-modal").length > 0) {
  		jQuery(".donorbox-modal").addClass("open");
    }
});

// close donorbox modal on clicking close button or anywhere outside of donation form
jQuery("body").click(function(e) {
  	if(e.target.className.includes("donorbox-modal-dimmed-bg") || e.target.className.includes("donorbox-modal-close-button")) {
        jQuery(".donorbox-modal").removeClass("open");
    }
});</script>
